{
  "application": {
    "name": "Application",
    "module": "base"
  },
  "modules": {
    "base": {
      "scripts": [
        {
          "src": "js/lib/jquery.js",
          "global": true
        },
        {
          "src": "js/lib/underscore.js",
          "global": true
        },
        {
          "src": "js/lib/backbone.js",
          "global": true
        },
        {
          "src": "js/lib/handlebars.js",
          "global": true
        },
        {
          "src": "js/lib/thorax.js",
          "global": true
        },
        {
          "src": "js/lib/thorax.event.js",
          "global": true
        },
        {
          "src": "js/lib/thorax.model.js",
          "global": true
        },
        {
          "src": "js/lib/thorax.collection.js",
          "global": true
        },
        {
          "src": "js/lib/thorax.helpers.js",
          "global": true
        },
        {
          "src": "js/lib/thorax.form.js",
          "global": true
        },
        {
          "src": "js/lib/thorax.view-controller.js",
          "global": true
        },
        {
          "src": "js/lib/thorax.loading.js",
          "global": true
        },
        {
          "src": "js/lib/script.js",
          "global": true
        },
        {
          "src": "js/lib/lumbar-loader.js"
        },
        {
          "src": "js/lib/lumbar-loader-events.js"
        },
        {
          "src": "js/lib/lumbar-loader-standard.js"
        },
        {
          "src": "js/lib/lumbar-loader-backbone.js"
        },
        {
          "src": "js/lib/thorax.lumbar.js"
        },
        {
          "src": "js/init.js"
        },
        {
          "src": "js/router.js"
        },
        {
          "src": "js/model.js"
        },
        {
          "src": "js/collection.js"
        },
        {
          "src": "js/view.js"
        },
        {
          "module-map": true
        },
        {
          "src": "js/lib/bootstrap.js"
        }
      ],
      "styles": [
        {
          "src": "styles/base.styl"
        },
        {
          "src": "styles/bootstrap.css"
        }
      ],
      "static": [
        {
          "src": "static/index.html",
          "dest": "index.html"
        }
      ]
    },
    "hello-world": {
      "routes": {
        "": "index"
      },
      "scripts": [
        {
          "src": "js/routers/hello-world.js"
        },
        {
          "src": "js/views/hello-world/index.js"
        }
      ],
      "styles": [
        {
          "src": "styles/hello-world.styl"
        }
      ]
    }
  },
  "templates": {
    "template": "Application.template('{{{name}}}', '{{{data}}}');",
    "js/init.js": [
      "templates/application.handlebars"
    ],
    "js/views/hello-world/index.js": [
      "templates/hello-world/index.handlebars"
    ]
  },
  "styles": {
    "pixelDensity": {
      "web": [
        1,
        2
      ]
    },
    "includes": [
      "nib"
    ]
  },
  "scope": {
    "template": "templates/module.handlebars"
  }
}