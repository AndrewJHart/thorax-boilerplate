{
  "application": {
    "name": "Application",
    "module": "base"
  },
  "modules": {
    "base": {
      "scripts": [
        {
          "src": "js/lib/zepto.js",
          "global": true
        },
        {
          "src": "js/lib/underscore.js",
          "global": true
        },
        {
          "src": "js/lib/backbone.js",
          "global": true
        },
        {
          "src": "js/lib/handlebars.js",
          "global": true
        },
        {
          "src": "js/lib/thorax-mobile.js",
          "global": true
        },
        {
          "src": "js/lib/script.js",
          "global": true
        },
        {
          "src": "js/lib/lumbar-loader.js"
        },
        {
          "src": "js/lib/lumbar-loader-events.js"
        },
        {
          "src": "js/lib/lumbar-loader-standard.js"
        },
        {
          "src": "js/lib/lumbar-loader-backbone.js"
        },
        {
          "src": "js/init.js"
        },
        {
          "src": "js/init-behaviors.js"
        },
        {
          "src": "js/view-controller.js"
        },
        {
          "src": "js/model.js"
        },
        {
          "src": "js/collection.js"
        },
        {
          "src": "js/view.js"
        },
        {
          "module-map": true
        }
      ],
      "styles": [
        {
          "src": "styles/base.styl"
        },
        {
          "src": "styles/bootstrap.css"
        },
        {
          "src": "styles/bootstrap-responsive.css"
        },
        {
          "src": "styles/loading-indicator.styl"
        }
      ],
      "static": [
        {
          "src": "static/index.html",
          "dest": "index.html"
        },
        {
          "src": "static/img",
          "dest": "img"
        }
      ]
    },
    "hello-world": {
      "routes": {
        "": "index"
      },
      "scripts": [
        {
          "src": "js/view-controllers/hello-world.js"
        },
        {
          "src": "js/views/hello-world/index.js"
        }
      ],
      "styles": [
        {
          "src": "styles/hello-world.styl"
        }
      ]
    },
    "examples": {
      "routes": {
        "examples/infinite-scroll": "infiniteScroll",
        "examples/fast-click": "fastClick",
        "examples/tap-highlight": "tapHighlight"
      },
      "scripts": [
        {
          "src": "js/view-controllers/examples.js"
        },
        {
          "src": "js/views/examples/infinite-scroll.js"
        },
        {
          "src": "js/views/examples/fast-click.js"
        },
        {
          "src": "js/views/examples/tap-highlight.js"
        }
      ],
      "styles": [
        {
          "src": "styles/examples.styl"
        }
      ]
    }
  },
  "templates": {
    "template": "Application.templates['{{{without-extension name}}}'] = '{{{data}}}';",
    "js/init.js": [
      "templates/application.handlebars"
    ],
    "js/views/hello-world/index.js": [
      "templates/hello-world/index.handlebars"
    ],
    "js/views/examples/infinite-scroll.js": [
      "templates/examples/infinite-scroll.handlebars"
    ],
    "js/views/examples/fast-click.js": [
      "templates/examples/fast-click.handlebars"
    ],
    "js/views/examples/tap-highlight.js": [
      "templates/examples/tap-highlight.handlebars"
    ]
  },
  "styles": {
    "pixelDensity": {
      "android": [ 1, 1.5 ],
      "iphone": [ 1, 2 ],
      "web": [ 1, 2 ]
    },
    "urlSizeLimit": 1500,
    "copyFiles": true,
    "styleRoot": "public",
    "includes": [
      "nib",
      "styles/include/global.styl"
    ]
  }
}
