{
  "application": {
    "name": "Application",
    "module": "base"
  },
  "modules": {
    "base": {
      "mixins": [
        "lumbar-loader",
        "thorax-jquery"
      ],
      "scripts": [
        {"src": "components/lumbar-loader/lumbar-loader-backbone.js"},
        {"src": "js/init.js"},
        {"src": "js/view-controller.js"},
        {"src": "js/model.js"},
        {"src": "js/collection.js"},
        {"src": "js/view.js"},
        {"src": "js/lib/bootstrap.js"}
      ],
      "styles": [
        {"src": "styles/base.styl"},
        {"src": "styles/bootstrap.css"}
      ],
      "static": [
        {"src": "static/index.html", "dest": "index.html"},
        {"src": "static/img", "dest": "img"}
      ]
    },
    "hello-world": {
      "routes": {
        "": "index"
      },
      "scripts": [
        {"src": "js/view-controllers/hello-world.js"},
        {"src": "js/views/hello-world/index.js"}
      ],
      "styles": [
        {"src": "styles/hello-world.styl"}
      ]
    }
  },
  "templates": {
    "js/init.js": [
      "templates/application.handlebars"
    ],
    "js/views/hello-world/index.js": [
      "templates/hello-world/index.handlebars"
    ]
  },
  "styles": {
    "pixelDensity": {
      "web": [
        1,
        2
      ]
    },
    "useNib": true
  },
  "mixins": [
    "components/lumbar-loader",
    "components/thorax"
  ]
}
